@model IEnumerable<CatAdoption.Models.Owner>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContactInfo.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ContactInfo.PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OwnerId }) |
            @Html.ActionLink("Details", "Details", new { id=item.OwnerId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.OwnerId })
        </td>
    </tr>
}

</table>
